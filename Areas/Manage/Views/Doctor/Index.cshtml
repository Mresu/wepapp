@using Medico.Models;
@model List<Doctors>

@{
    int count = 0;
}


<h5>
        doctors
    </h5>

    <a class="btn btn-secondary " asp-action="Create">Create</a>
<div class="container">

    <table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">FullName</th>
      <th scope="col">Position</th>
      <th scope="col">Image</th>
    </tr>
  </thead>
  <tbody>
            @foreach (var item in Model)
            {
                count++;
          <tr>
          <th scope="row">@count</th>
          <td>@item.FullName</td>
          <td>@item.Position</td>
           <td><img src="~/limon/doctorlimon"></img></td>

        </tr>
            }


  </tbody>
</table>
</div>